{
  "name": "ringierimu/experiments",
  "description": "SDC Experiments Package ðŸ§ª",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Gareth Edwards",
      "email": "tools@ringier.co.za"
    }
  ],
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Ringierimu\\Experiments\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ringierimu\\Experiments\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ringierimu\\Experiments\\ExperimentsServiceProvider"
      ],
      "aliases": {
        "SdcExperiments": "Ringierimu\\Experiments\\Facades\\SdcExperiments"
      }
    }
  },
  "require": {
    "php": "^7.2|^8.0",
    "ext-json": "*",
    "illuminate/support": "^6|^7|^8|^9",
    "spatie/laravel-googletagmanager": "^2.5"
  },
  "require-dev": {
    "mockery/mockery": "1.5.0",
    "phpunit/phpunit": "9.5.22",
    "orchestra/testbench": "7.6.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
